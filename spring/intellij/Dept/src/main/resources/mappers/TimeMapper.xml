<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.app.manager.mapper.TimeMapper"> <!--인터페이스 풀네임으로 정의-->

    <!-- id = "메소드의 이름과 일치시키기" resultType = "반환 타입" -->
